     </action>
     <action name="delete" class="br.com.sysmapaweb.action.EstabelecimentoAction" method="delete" >
       <result name="success">views/estabelecimentos.jsp</result>       
     </action>
     <action name="edit" class="br.com.sysmapaweb.action.EstabelecimentoAction" method="edit" >
